{
  "name": "@blueeast/bluerain-platform-react-native",
  "version": "1.1.3",
  "description": "Bluerain-OS Platform for react-native",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/bluerain-platform-react-native.d.ts",
  "build-storybook": "build-storybook -s src -o .out",
  "files": [
    "dist"
  ],
  "release": {
    "branch": "master",
    "dryRun": false,
    "debug": true
  },
  "author": "BlueEast Team <code@blueeast.com>",
  "repository": {
    "type": "git",
    "url": "http:git@github.com:BlueEastCode/bluerain-platform-react-native.git",
    "dry-run": false
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "rm -rf dist && bluerain build plugin",
    "start": "tsc -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls && codecov",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --use-tabs --single-quote",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 40,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverage": true,
    "mapCoverage": true
  },
  "devDependencies": {
    "@blueeast/bluerain-cli": "^3.0.0",
    "@blueeast/bluerain-cli-plugin": "^0.1.3",
    "@blueeast/bluerain-ui-interfaces": "^3.2.9",
    "@blueeast/tslint-config-blueeast": "^0.5.1",
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.0",
    "@types/react": "^16.3.14",
    "babel-cli": "^6.26.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.0",
    "jest": "^21.0.0",
    "lint-staged": "^6.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "react-native": "^0.56.0",
    "semantic-release": "^14.0.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.0.1",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.9.0",
    "typescript": "2.7.1",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "@blueeast/bluerain-os": "^3.13.4",
    "immutable": "^3.8.2",
    "is-electron": "^2.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "synctasks": "^0.3.2"
  },
  "peerDependencies": {
    "@blueeast/bluerain-os": "3.x.x",
    "react": "16.x.x",
    "react-dom": "16.x.x"
  }
}
